<tr id='{{address}}'>
    <td class='address'>{{address}}</td>
    <td class='net'>{{is_online}}</td>
    <td><a href='https://{{address}}' class='model' target='_BLANK'>{{model}}</a></td>
    <td class='status'>{{status}}</td>
    <td class='user'>{{reservedby}}</td>
    <td class='actions' name='actions'>
    <button class="btn btn-info" type="submit">{{action}}</button>
    </td>
</tr>